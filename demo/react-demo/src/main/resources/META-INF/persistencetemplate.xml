<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
 http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd" version="1.0">
 
   <persistence-unit name="DOECode">
     <description>Persistence Unit containing DOE Code entities</description>
     <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
     <class>gov.osti.entity.Agent</class>
     <class>gov.osti.entity.ContributingOrganization</class>
     <class>gov.osti.entity.Contributor</class>
     <class>gov.osti.entity.Developer</class>
     <class>gov.osti.entity.DOECodeMetadata</class>
     <properties>
       <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
       <property name="javax.persistence.jdbc.url" value="url"/>
       <property name="javax.persistence.jdbc.user" value=""/>
       <property name="javax.persistence.jdbc.password" value=""/>
       <property name="hibernate.hbm2ddl.auto" value="create-drop"/>
     </properties>
   </persistence-unit>
 
</persistence>